{
  "name": "over-collateral",
  "main": "index.js",
  "version": "1.0.0",
  "license": "GPL-3.0",
  "private": true,
  "duet": {
    "sdk": true
  },
  "scripts": {
    "compile": "npx hardhat compile",
    "compile:watch": "npx hardhat watch compile",
    "clean": "rm -rf artifacts && rm -rf cache && rm -rf coverage && rm -rf typechain",
    "deploy:bsc": "npx hardhat deploy --network bsc && ts-node -T ./scripts/clean-solc-inputs.ts",
    "deploy:bsctest": "npx hardhat deploy --network bsctest && ts-node -T ./scripts/clean-solc-inputs.ts",
    "deploy:local": "npx hardhat deploy --network local",
    "verify:bsc": "npx hardhat --network bsc etherscan-verify && npx hardhat --network bsc verify:duet",
    "verify:bsctest": "npx hardhat --network bsctest etherscan-verify && npx hardhat --network bsctest verify:duet",
    "simulate:20220715:deploy:forkedbsctest": "FORK_URL__BSCTEST='https://rpc.tenderly.co/fork/65bc8c53-036c-4fbc-99ee-1f4b0763d164' npm run deploy:bsctest",
    "simulate:20220715:run:forkedbsctest": "FORK_URL__BSCTEST='https://rpc.tenderly.co/fork/65bc8c53-036c-4fbc-99ee-1f4b0763d164' npx hardhat --network bsctest duet:accident-20220715:users",
    "simulate:20220715:prepare:forkedbsc": "FORK_URL__BSC='https://rpc.tenderly.co/fork/1388a757-8bed-4e22-a86d-d1b6330140cc' npx hardhat --network bsc duet:accident-20220715:prepare",
    "simulate:20220715:deploy:forkedbsc": "FORK_URL__BSC='https://rpc.tenderly.co/fork/1388a757-8bed-4e22-a86d-d1b6330140cc' NO_MULTISIG=1 npm run deploy:bsc",
    "simulate:20220715:run:forkedbsc": "FORK_URL__BSC='https://rpc.tenderly.co/fork/1388a757-8bed-4e22-a86d-d1b6330140cc' npx hardhat --network bsc duet:accident-20220715:users",
    "abi": "npx hardhat export-abi",
    "hardhat": "npx hardhat",
    "hint": "npx hardhat check",
    "test": "REPORT_GAS=true npx hardhat test",
    "test:node": "npx hardhat node",
    "test:watch": "npx hardhat watch test",
    "test:coverage": "npx hardhat coverage",
    "test:size-contracts": "npx hardhat size-contracts",
    "docgen": "rm -rf ./docs && npx hardhat docgen",
    "prettier": "prettier --write 'contracts/**/*.sol'"
  },
  "dependencies": {
    "@chainlink/contracts": "0.4.0",
    "@openzeppelin/contracts": "4.3.2",
    "@openzeppelin/contracts-upgradeable": "^4.4.0",
    "@uniswap/lib": "^4.0.1-alpha",
    "@uniswap/v2-core": "^1.0.1"
  },
  "devDependencies": {
    "@private/shared": "workspace:*",
    "axios": "^0.27.2"
  }
}
